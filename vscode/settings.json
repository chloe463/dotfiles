{
  "editor.fontFamily": "HackGen Console",
  "editor.formatOnSave": true,
  "vim.easymotion": true,
  "vim.sneak": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    },
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    },
    {
      "before": ["k", "j"],
      "after": ["<Esc>"]
    },
    {
      "before": ["k", "k"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "d"],
      "after": ["d", "d"]
    },
    {
      "before": ["<C-n>"],
      "commands": [":nohl"]
    }
  ],
  "vim.leader": "<space>",
  "breadcrumbs.enabled": true,
  "flow.useNPMPackagedFlow": true,
  "workbench.editor.showTabs": true,
  "editor.renderWhitespace": "all",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "todohighlight.isEnable": true,
  // "todohighlight.keywords": ["TODO(chloe463):", "NOTE", "HACK", "!todo"],
  "todohighlight.keywords": [
    "FIXME",
    {
      "text": "NOTE",
      "color": "#ffffff",
      "backgroundColor": "#08C5E7",
      "overviewRulerColor": "grey"
    },
    {
      "text": "HACK",
      "color": "#000",
      "isWholeLine": false,
    },
    {
      "text": "TODO(chloe463)",
      "color": "white",
      "backgroundColor": "#f65948",
    },
    "TODO"
  ],
  "todohighlight.exclude": [
    "**/node_modules/**",
    "**/bower_components/**",
    "**/dist/**",
    "**/build/**",
    "**/.vscode/**",
    "**/.github/**",
    "**/_output/**",
    "**/*.min.*",
    "**/*.map",
    "**/.next/**",
    "**/vendor/**",
    "**/tmp/**",
    "**/log/**"
  ],
  "workbench.colorTheme": "Nord",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.fontFamily": "HackGen Console",
  "terminal.integrated.fontSize": 14,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.tabSize": 2,
  "eslint.alwaysShowStatus": true,
  "eslint.packageManager": "yarn",
  "editor.fontWeight": "100",
  "liveshare.featureSet": "insiders",
  "ruby.rubocop.useBundler": true,
  "editor.fontSize": 14,
  "files.exclude": {
    "/.git": true,
    "/.DS_Store": true,
    "/node_modules": true,
    "/node_modules/": true
  },
  "search.exclude": {
    "/node_modules": true
  },
  "files.watcherExclude": {
    "/node_modules/": true
  },
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.codeLens.enabled": false,
  "gitlens.statusBar.enabled": false,
  "gitlens.defaultDateStyle": "absolute",
  "editor.codeActionsOnSave": {
    "source.organizeImports": true,
    "source.fixAll": true,
    "source.fixAll.eslint": true, // eslint
    "source.fixAll.stylelint": true // Stylelint
  },
  "peacock.favoriteColors": [
    {
      "name": "Angular Red",
      "value": "#dd0531"
    },
    {
      "name": "Azure Blue",
      "value": "#007fff"
    },
    {
      "name": "JavaScript Yellow",
      "value": "#f9e64f"
    },
    {
      "name": "Mandalorian Blue",
      "value": "#1857a4"
    },
    {
      "name": "Node Green",
      "value": "#215732"
    },
    {
      "name": "React Blue",
      "value": "#61dafb"
    },
    {
      "name": "Something Different",
      "value": "#832561"
    },
    {
      "name": "Svelte Orange",
      "value": "#ff3d00"
    },
    {
      "name": "Vue Green",
      "value": "#42b883"
    }
  ],
  "peacock.affectTitleBar": false,
  "eslint.run": "onSave",
  "files.insertFinalNewline": true,
  "js/ts.implicitProjectConfig.experimentalDecorators": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "workbench.iconTheme": "material-icon-theme",
  "rust-client.rustupPath": "/Users/Tsuyoshi/.cargo/bin/rustup",

  "editor.matchBrackets": "near",
  "editor.bracketPairColorization.enabled": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDragAndDrop": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "window.zoomLevel": 1,
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": true,
    "markdown": false,
    "typescriptreact": false,
    "ruby": true,
    "typescript": true,
    "jsonc": true,
    "haml": true,
    "javascript": false,
    "sql": false
  }
  // "editor.cursorStyle": "line",
  // "editor.insertSpaces": false,
  // "editor.lineNumbers": "on",
  // "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  // "editor.wordWrap": "off"
}
